[asterisk]
enabled       = true
backend       = polling
journalmatch  =
maxretry      = 3
filter        = asterisk
logpath       = /var/log/asterisk/messages.log
action        = nftables-allports[name="%(__name__)s-tcp", protocol="tcp"]
                nftables-allports[name="%(__name__)s-udp", protocol="udp"]
